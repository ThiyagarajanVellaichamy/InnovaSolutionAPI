@model Killark.Models.DashboardModel
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/Content/HubbellCustom/Script/ajaxCommon.js"></script>
<script src="~/Content/HubbellCustom/Script/dashboard.js"></script>
<link href="~/Content/css/font-awesome.min.css" rel="stylesheet">
<body onload="loadDashboardData()">
    <div id="dialog" title="Basic dialog">
        <p>It's not support for this view. Please view in desktop</p>
    </div>
    <div>
        <div>
            <div class="kil-bdy-wrap kil-inpbdy-wrap">
                <!--Modal Box for Application Selection start-->
                <div class="top-sec clearfix">
                    <div class="butn clearfix">
                        <button type="button" class="kil-btn kil-btn-small btf"
                                data-toggle="modal" data-target="#myModal">
                            New Design
                        </button>
                    </div>
                    <!-- Modal -->
                    <div class="modal fade tileModal" id="myModal" role="dialog">
                        <h3 class="tile-head">Select Application</h3>
                        <div class="tile-body-wrapper">
                            <div class="tile-box">
                                <input type="radio" id="control_01" name="appName" value="EXBLT" checked>
                                <label for="control_01">
                                    <h2 class="tile-option-head">EXBLT</h2>
                                </label>
                            </div>
                            <div class="tile-box">
                                <input type="radio" id="control_02" name="appName" value="EXB">
                                <label for="control_02">
                                    <h2 class="tile-option-head">EXB</h2>
                                </label>
                            </div>
                        </div>
                        <div class="tile-footer">
                            <button class="kil-btn kil-btn-small go-btn">
                                Go
                                <span class="glyphicon glyphicon-arrow-right icon-arrow"></span>
                            </button>
                        </div>
                    </div>
                    <div class="search-container clearfix">
                        <div class="search-inner">
                            <input id="inputSearchField" type="text" placeholder="Search" name="search">
                        </div>
                    </div>
                </div>
                <!--Modal Box for Application Selection end-->
                <!-- Table starts -->
                <div class="clearfix dash-board hc-info-data">
                    <div class="table-responsive scrollstyle">
                        <table id="dash-boardtb" class="table">
                            <thead>
                                <tr>
                                    <th class="no-row">
                                        <a href="javascript:void(0)" class="nmb-image">
                                            <img src="~/Content/images/tb1.png"
                                                 alt="order-arrow" title="Arrow"
                                                 onclick="w3.sortHTML('#dash-boardtb', '.item', 'td:nth-child(2)')">
                                        </a>
                                    </th>
                                    <th>ID</th>
                                    <th>Product Type</th>
                                    <th>
                                        <span>Time Stamp</span><a href="javascript:void(0)" class="arrow-image">
                                            <img src="~/Content/images/tarow.png" alt="order-arrow" title="Sort"
                                                 onclick="w3.sortHTML('#dash-boardtb', '.item', 'td:nth-child(4)')">
                                        </a>
                                    </th>
                                    <th>Download</th>
                                    <th>Status</th>
                                    <th>Remarks</th>
                                </tr>
                            </thead>
                            <tbody id="dash-boardtable"></tbody>
                        </table>
                    </div>
                </div>
                <!-- Table ends -->
            </div>
        </div>
    </div>
</body>